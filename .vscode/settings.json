{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "node_modules": true,
    ".cache": true,
    "build": true,
    "public/build": true
  },
  "editor.formatOnSave": true,
  "editor.foldingStrategy": "indentation",
  "files.insertFinalNewline": true,
  "files.autoSave": "onWindowChange"
}
